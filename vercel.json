{
  "buildCommand": "echo 'Static SPA - no build'",
  "outputDirectory": ".",
  "public": true,
  "cleanUrls": false,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/:path*",
      "destination": "/:path*"
    },
    {
      "source": "/((?!api).*)",
      "destination": "/10-APP-index.html"
    }
  ]
}
